{
	"name": "icanboogie/message-bus",
	"type": "library",
	"description": "A message bus to handle or push messages",
	"keywords": [
		"message",
		"bus"
	],
	"homepage": "https://icanboogie.org/",
	"license": "BSD-3-Clause",
	"authors": [
		{
			"name": "Olivier Laviale",
			"email": "olivier.laviale@gmail.com",
			"homepage": "https://olvlvl.com/",
			"role": "Developer"
		}
	],
	"support": {
		"issues": "https://github.com/ICanBoogie/MessageBus/issues",
		"source": "https://github.com/ICanBoogie/MessageBus"
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"prefer-dist": true,
	"require": {
		"php": "^7.2",
		"icanboogie/accessor": "^2.0"
	},
	"require-dev": {
		"psr/container": "^1.0",
		"symfony/dependency-injection": "^4.0",
		"symfony/config": "^4.0",
		"symfony/yaml": "^4.0"
	},
	"autoload": {
		"psr-4": {
			"ICanBoogie\\MessageBus\\": "lib/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"ICanBoogie\\MessageBus\\": "tests/"
		}
	},
	"extra": {
		"icanboogie": {
			"config-path": "config"
		}
	}
}
